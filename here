const handleNodeClick = (node) => {
  if (node.children && node.children.length > 0) {
    // Clicked on a level 1 node, keep track of selectedNodes
    setSelectedNodes([node]);
  } else {
    // Clicked on a level 2 node, perform redirection
    // Replace the following line with your actual redirection logic
    // For example, you can use React Router's history.push() if you are using it
    // history.push(node.href);

    // For simplicity, this example uses window.location.href
    window.location.href = node.href;
  }
};
