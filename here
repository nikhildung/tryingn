<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>jsTree Example with jQuery</title>
    <!-- Include jQuery and jsTree CSS and JS -->
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jstree/3.3.12/themes/default/style.min.css" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jstree/3.3.12/jstree.min.js"></script>
</head>
<body>

<!-- Container for jsTree -->
<div id="tree"></div>

<script>
    // Sample JSON data
    var jsonData = [
        {
            "id": "node1",
            "parent": "#",
            "text": "Root Node 1"
        },
        {
            "id": "node2",
            "parent": "#",
            "text": "Root Node 2"
        },
        {
            "id": "node3",
            "parent": "#",
            "text": "Root Node 3"
        },
        {
            "id": "node4",
            "parent": "node1",
            "text": "Child Node 1"
        },
        {
            "id": "node5",
            "parent": "node1",
            "text": "Child Node 2"
        },
        {
            "id": "node6",
            "parent": "node2",
            "text": "Child Node 3"
        },
        {
            "id": "node7",
            "parent": "node2",
            "text": "Child Node 4"
        },
        

    ];


    $(document).ready(function () {
        $('#tree').jstree({
            'core': {
                'data': jsonData
            }
        });
    });

</script>

</body>
</html>
