import React from 'react';
import JSONTree from 'react-json-tree';

const data = [
  // ... (Your JSON data here)
];

const TreeView = () => {
  const treeStyle = {
    marginTop: '20px',
    marginLeft: '20px',
  };

  return (
    <div style={treeStyle}>
      <JSONTree data={data} theme="monokai" invertTheme={false} />
    </div>
  );
};

export default TreeView;
